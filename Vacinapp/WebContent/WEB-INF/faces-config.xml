<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

	<navigation-rule>
		<from-view-id></from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastro</from-outcome>
			<to-view-id>/paginas/cadastro.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>minhasVacinas</from-outcome>
			<to-view-id>/paginas/usuario/minhasVacinas.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>vacinas</from-outcome>
			<to-view-id>/paginas/vacinas.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>locais</from-outcome>
			<to-view-id>/paginas/locais.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>configuracao</from-outcome>
			<to-view-id>/paginas/configuracao.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>solicitacoes</from-outcome>
			<to-view-id>/paginas/agente/solicitacoes.jsf</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
